{
  "name": "ng-test",
  "version": "1.1.0",
  "description": "A webpack starter for Angular",
  "main": "server/server.js",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development node server/server.js",
    "pack": "webpack-dev-server --inline --progress --port 8080",
    "test": "karma start",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config config/webpack.prod.js --progress --profile --bail",
    "production": "npm run build && node server/product-server.js",
    "changelog:all": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "changelog": "conventional-changelog -p atom -i CHANGELOG.md -s",
    "lint": "tslint src/**/*.ts",
    "fix": "tslint src/**/*.ts --fix",
    "commit:all": "git add . & cz",
    "commit": "cz"
  },
  "keywords": [
    "angular",
    "typescript",
    "ts",
    "webpack",
    "es6"
  ],
  "author": "",
  "dependencies": {
    "@angular/animations": "^9.0.0",
    "@angular/common": "^9.0.0",
    "@angular/compiler": "^9.0.0",
    "@angular/core": "^9.0.0",
    "@angular/forms": "^9.0.0",
    "@angular/platform-browser": "^9.0.0",
    "@angular/platform-browser-dynamic": "^9.0.0",
    "@angular/router": "^9.0.0",
    "core-js": "^3.0.1",
    "normalize.css": "8.0.1",
    "rxjs": "^6.5.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^9.0.0",
    "@angularclass/hmr": "2.1.3",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-angular": "^11.0.0",
    "@ngtools/webpack": "^9.0.1",
    "@types/jasmine": "3.3.13",
    "@types/node": "12.0.3",
    "angular-hot-reload-loader": "1.0.2",
    "angular2-template-loader": "0.6.2",
    "autoprefixer": "9.5.1",
    "awesome-typescript-loader": "5.2.1",
    "codelyzer": "5.2.1",
    "commitizen": "^4.2.2",
    "connect-history-api-fallback": "1.6.0",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "5.2.0",
    "css-loader": "2.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "enhanced-resolve": "4.1.0",
    "eventsource-polyfill": "0.9.6",
    "express": "4.17.1",
    "express-favicon": "2.0.1",
    "file-loader": "^3.0.1",
    "html-loader": "0.5.5",
    "html-minify-loader": "1.4.0",
    "html-webpack-plugin": "3.2.0",
    "http-proxy-middleware": "0.19.1",
    "husky": "1.3.1",
    "ip": "1.1.5",
    "jasmine-core": "3.4.0",
    "karma": "4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "2.0.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "3.0.5",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^0.7.0",
    "ng-router-loader": "2.1.0",
    "node-sass": "^4.8.3",
    "null-loader": "2.0.0",
    "open": "6.3.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss": "7.0.16",
    "postcss-loader": "3.0.0",
    "prettier": "2.2.1",
    "rimraf": "2.6.3",
    "sass-loader": "^7.1.0",
    "standard-version": "^9.0.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "to-string-loader": "1.1.5",
    "tslint": "^5.2.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-formatter-eslint-style": "0.0.1",
    "tslint-loader": "3.6.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "3.7.5",
    "url-loader": "1.1.2",
    "webpack": "~4.32.2",
    "webpack-cli": "~3.3.2",
    "webpack-dev-middleware": "3.7.0",
    "webpack-dev-server": "3.4.1",
    "webpack-filter-warnings-plugin": "^1.2.0",
    "webpack-hot-middleware": "2.25.0",
    "webpack-merge": "4.2.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged & npm run lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "tslint --fix",
      "git add"
    ]
  }
}
